{
  "name": "react101",
  "version": "1.0.0",
  "description": "Getting started with React",
  "scripts": {
    "start": "npm-run-all --parallel start-dev-api start-dev-ui",
    "dev": "webpack-dev-server -w --hot --inline --debug-info",
    "deploy": "webpack -p --config webpack.production.config.js",
    "s": "webpack -w --progress --colors",
    "start-dev-api": "json-server --watch api/db.json --routes api/routes.json --port 5000",
    "start-dev-ui": "webpack-dev-server"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.1.4",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.4.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.7"  
  },
  "devDependencies": {
    "json-server": "^0.8.14",
    "standard": "^7.0.0",
    "npm-run-all": "^1.8.0",
    "webpack-dev-server": "^2.0.0-beta"

  }
}
